{
  "manifest_version": 2,
  "name": "Jira",
  "description": "An extension to make quick Jira jumps via the omnibox",
  "version": "2.0.1",
  "omnibox": {
    "keyword": "j"
  },
  "icons": {
    "16": "jira16.png",
    "32": "jira32.png",
    "48": "jira48.png",
    "64": "jira64.png",
    "128": "jira128.png"
  },
  "options_page": "options.html",
  "browser_action": {
    "default_title": "Issues",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "cookies",
    "storage",
    "https://*.atlassian.net/"
  ],
  "optional_permissions": [
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "*://*/*"
  ]
}
